#!/bin/csh
if ($#argv != 5) then
  echo "USAGE: C_plot_westUS_wk [data infile] [lon1, lon2, lat1, lat2]"
  exit 1
endif
set input_map = $argv[1]
#set REG = -R235/270/25/50
set lon1 = $2
set lon2 = $3
set lat1 = $4
set lat2 = $5

set REG = -R${lon1}/${lon2}/${lat1}/${lat2}
gmtset BASEMAP_TYPE fancy
awk '{if ($7>0) print $1,$2,$3}' $input_map | surface -T0.2 -Gtomo.grd -I0.2 $REG
grd2xyz tomo.grd $REG > $input_map".HD_0.2"

